<% include ./variables/common.ejs %>
<% include ./variables/mypage_cpa_accessRoute.ejs %>
<% include ./partials/header.ejs %>
<% include ./partials/nav.ejs %>

<section class="container full-width clearfix mypage">
  <h2 class="main-h">마이페이지</h2>
  <div class="cols">
    <% include ./partials/mypage_cpa_mycampaign/left_col.ejs %>
    <div class="col-9 right-col">
      <h2>접속경로</h2>
      <div class="search">
        <div class="options">
          <div class="items">
            <div class="selected">
              <button type="button" class="selected-button">
                <span class="value">전체</span>
                <img class="arrow-down" src="data:image/svg+xml;charset=utf8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 64 64' aria-labelledby='title' aria-describedby='desc' role='img' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Ctitle%3EAngle Down%3C/title%3E%3Cdesc%3EA line styled icon from Orion Icon Library.%3C/desc%3E%3Cpath data-name='layer1' fill='none' stroke='%235c5c5c' stroke-miterlimit='10' stroke-width='1' d='M20 26l11.994 14L44 26' stroke-linejoin='round' stroke-linecap='round'%3E%3C/path%3E%3C/svg%3E" alt="Angle Down" />
              </button>
            </div>
            <ul>
              <li class="list-item">전체</li>
            </ul>
          </div>
        </div>

        <div class="options">
          <div class="items">
            <div class="selected">
              <button type="button" class="selected-button">
                <span class="value">전체</span>
                <img class="arrow-down" src="data:image/svg+xml;charset=utf8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 64 64' aria-labelledby='title' aria-describedby='desc' role='img' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Ctitle%3EAngle Down%3C/title%3E%3Cdesc%3EA line styled icon from Orion Icon Library.%3C/desc%3E%3Cpath data-name='layer1' fill='none' stroke='%235c5c5c' stroke-miterlimit='10' stroke-width='1' d='M20 26l11.994 14L44 26' stroke-linejoin='round' stroke-linecap='round'%3E%3C/path%3E%3C/svg%3E" alt="Angle Down" />
              </button>
            </div>
            <ul>
              <li class="list-item">전체</li>
            </ul>
          </div>
        </div>

        <div class="datepicker from">
          <input type="text" id="datepicker-from">
          <img src="/static/images/mypage/mypage_calendar.png">
        </div>
        <span class="wave">~</span>
        <div class="datepicker to">
          <input type="text" id="datepicker-to">
          <img src="/static/images/mypage/mypage_calendar.png">
        </div>
        <button class="period today"> 오늘 </button>
        <button class="period cur-month"> 이번달 </button>
        <button class="period prev-month"> 저번달 </button>
        <button class="inquiry"> 조회 </button>
      </div>
      <div class="wrapper-table">
        <div class="status-dashboard clearfix">
          <div class="col number">
            <ul class="clearfix">
              <li class="wait">
                <span class="square wait"></span>
                <span class="text">
                대기
              </span>
              </li>
              <li class="defer">
                <span class="square defer"></span>
                <span class="text">
                보류
              </span>
              </li>
              <li class="approve">
                <span class="square approve"></span>
                <span class="text">
                승인
              </span>
              </li>
              <li class="deny">
                <span class="square deny"></span>
                <span class="text">
                  거절
                </span>
              </li>
              <li class="visited">
                <span class="square visited"></span>
                <span class="text">
                  방문
                </span>
              </li>
            </ul>
          </div>
        </div>
        <table class="table mobile">
          <thead>
            <tr>
              <th class="th-date">날짜</th>
              <th>캠페인명/아이피</th>
              <th>상태</th>
              <th>유입경로</th>
              <!-- <th>접속기기</th> -->
            </tr>
          </thead>
          <tbody>
            <% for (i = 0; i < 10; i++) { %>
              <tr>
                <td class="date">05.05</td>
                <td class="campaign-name">
                  캠페인명일 길다고 가정했을때 <br>
                  <span>(110.xxx.xxx.67)</span>
                </td>
                <!-- <td class="remote-ip">110.xxx.xxx.67</td> -->
                <td class="status">
                  <% if (i % 5 === 4) { %>
                    <span class="defer"></span>
                  <% } %>
                  <% if (i % 5 === 3) { %>
                    <span class="wait"></span>
                  <% } %>
                  <% if (i % 5 === 2) { %>
                    <span class="approve"></span>
                  <% } %>
                  <% if (i % 5 === 1) { %>
                    <span class="deny"></span>
                  <% } %>
                  <% if (i % 5 === 0) { %>
                    <span class="visited"></span>
                  <% } %>
                </td>
                <td class="access-route">
                  <% if (i % 2 === 0) { %>
                    <a target="_blank" href="http://www.rainy-heaven.com">
                      www.rainy-heaven.comwww.
                    </a>
                  <% } %>
                  <% if (i % 2 === 1) { %>
                    직접입력
                  <% } %>
                </td>
                <!-- <td class="access-device">
                  <% if (i % 2 === 0) { %>
                    PC
                  <% } %>

                  <% if (i % 2 === 1) { %>
                    Mobile
                  <% } %>
                </td> -->
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>

      <div class="pagination">
        <div class="col icon prev-last">
          <img src="/static/images/mypage/my_page_numbering_left_end.png" alt="처음페이지">
        </div>
        <div class="col icon prev">
          <img src="/static/images/mypage/my_page_numbering_left.png" alt="이전페이지">
        </div>
        <div class="col active">
          1
        </div>
        <div class="col">
          2
        </div>
        <div class="col">
          3
        </div>
        <div class="col">
          4
        </div>
        <div class="col">
          5
        </div>
        <div class="col icon next">
          <img src="/static/images/mypage/my_page_numbering_right.png" alt="다음페이지">
        </div>
        <div class="col icon next-last">
          <img src="/static/images/mypage/my_page_numbering_right_end.png" alt="끝페이지">
        </div>
      </div>

      <% include ./partials/mypage_cpa_mycampaign/submenu.ejs %>
      
    </div>
  </div>
</section>
<% include ./partials/footer.ejs %>
