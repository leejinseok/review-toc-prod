<!-- aside_bottom.html -->
<aside class="bottom">
  <div class="inner clearfix">
    <div class="notice col">
      <div class="header">
        <h3>
          공지사항
          <a href="#" class="more">
            More +
          </a>
        </h3>
      </div>
      <table>
      <% for (i = 0; i < notices.length; i++) { %>
        <tr>
          <td class="title">
            <a href="#">
              <%= notices[i].title %>
            </a>
          </td>
          <td class="date">
            <%= notices[i].date %>
          </td>
        </tr>
      <% } %>
      </table>
    </div>
    <div class="rank col">
      <div class="header">
        <h3>
          전월 승인률별 순위
        </h3>
      </div>
      <ul>
        <% for(i = 0; i < ranks.length; i++) { %>
          <li>
            <span class="circle-number"> <%= (i + 1) %> </span>
            <a href="#" title="<%= ranks[i]['title'] %>"><%= ranks[i]['title'] %></a>
          </li>
        <% } %>
      </ul>
    </div>
    <div class="guide-contact col">
      <div class="left">
        <ul>
          <li class="menu">
            <a href="/service_guide.html">
              <img src="./static/images/chat.svg" class="symbol chat" style="">
